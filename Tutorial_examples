started = False
while 1:
    i=input('> ')
    if i == 'start':
        if started:
            print('car already started')
        else:
            started=True
            print('car started')
    elif i == 'help':
        print('help details')
    elif i =='stop':
        print('car stopped')
    elif i == 'quit':
        print('quit')
        break
    else:
        print("i don't understand")
