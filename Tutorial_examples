    ":)": "ðŸ˜",
    ":(": "ðŸ˜”",
}
output= ""
for word in words:
    output += emojis.get(word,  word)+ " "
print(output)
----------------------

def emoji_convert(message):
    words = message.split(" ")
    emojis = {
        ":)" :"ðŸ˜",
        ":(" : "ðŸ˜”",
    }
    output = ''
    for word in words:
        output +=emojis.get(word, word)+" "
    return output


message = input(">")
print(emoji_convert(message))
