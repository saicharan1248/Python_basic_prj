#minmax
def miniMaxSum(arr):
    min_ =float('inf')
    max_ =float('-inf')
    for i in range(len(arr)):
        current = arr[i]
        arr[i] =0
        total =sum(arr)
        max_=max(max_,total)
        min_=min(min_,total)
        arr[i]= current
    return min_ , max_

if __name__ == '__main__':

    arr = list(map(int, input().rstrip().split()))

    result = miniMaxSum(arr)
    print(*result)nmax
