# hackerrank list concept:
if __name__ == '__main__':
    N = int(input())
    list_=[ ]
    while N!=0:
        command, *args = input().split()
        if command=='insert':
            i = int(args[0])
            e = int(args[1])
            list_.insert(i,e)
        elif command=='print':
            print(list_)
        elif command == 'remove':
            e=int(args[0])
            list_.remove(e)
        elif command == 'append':
            e =int(args[0])
            list_.append(e)
        elif command == 'sort':
            list_.sort()
        elif command == 'pop':
            list_.pop()
        elif command == 'reverse':
            list_.reverse()
        N=N-1
